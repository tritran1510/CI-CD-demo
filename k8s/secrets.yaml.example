# Copy this file to secrets.yaml and add your actual values
# DON'T commit secrets.yaml to git!
apiVersion: v1
kind: Secret
metadata:
  name: express-server-secrets
type: Opaque
stringData:
  mongo-uri: "mongodb+srv://username:password@cluster.mongodb.net/dbname"
  jwt-secret: "your-super-secret-jwt-key"
  refresh-token-secret: "your-refresh-token-secret"
